obal.analytic</ref> (<hi rend="attribute">@ana</hi>))  (<ref target="#TEI.att.global.responsibility">att.global.responsibility</ref> (<hi rend="attribute">@cert</hi>, <hi rend="attribute">@resp</hi>))  (<ref target="#TEI.att.global.source">att.global.source</ref> (<hi rend="attribute">@source</hi>)) <ref target="#TEI.att.datable">att.datable</ref> (<hi rend="attribute">@calendar</hi>, <hi rend="attribute">@period</hi>)<c xml:space="preserve"> </c> (<ref target="#TEI.att.datable.w3c">att.datable.w3c</ref> (<hi rend="attribute">@when</hi>, <hi rend="attribute">@notBefore</hi>, <hi rend="attribute">@notAfter</hi>, <hi rend="attribute">@from</hi>, <hi rend="attribute">@to</hi>))  (<ref target="#TEI.att.datable.iso">att.datable.iso</ref> (<hi rend="attribute">@when-iso</hi>, <hi rend="attribute">@notBefore-iso</hi>, <hi rend="attribute">@notAfter-iso</hi>, <hi rend="attribute">@from-iso</hi>, <hi rend="attribute">@to-iso</hi>))  (<ref target="#TEI.att.datable.custom">att.datable.custom</ref> (<hi rend="attribute">@when-custom</hi>, <hi rend="attribute">@notBefore-custom</hi>, <hi rend="attribute">@notAfter-custom</hi>, <hi rend="attribute">@from-custom</hi>, <hi rend="attribute">@to-custom</hi>, <hi rend="attribute">@datingPoint</hi>, <hi rend="attribute">@datingMethod</hi>)) <ref target="#TEI.att.editLike">att.editLike</ref> (<hi rend="attribute">@evidence</hi>, <hi rend="attribute">@instant</hi>)<c xml:space="preserve"> </c><table rend="attList"><row><cell rend="odd_label">level</cell><cell rend="odd_value"><table rend="attDef"><row><cell rend="odd_label"><hi rend="label" xml:lang="en">Status</hi> </cell><cell rend="odd_value"><seg xml:lang="en">Optional</seg></cell></row><row><cell rend="odd_label"><hi xml:lang="en" rend="label">Datatype</hi> </cell><cell rend="odd_value"><tei:ref xmlns:tei="http://www.tei-c.org/ns/1.0" target="#TEI.teidata.enumerated">teidata.enumerated</tei:ref></cell></row><row><cell rend="odd_label"><hi rend="label" xml:lang="en">Legal values are:</hi></cell><cell rend="odd_value"><list rend="valList"><label rend="odd_label">low</label><item rend="odd_value"/><label rend="odd_label">medium</label><item rend="odd_value"/><label rend="odd_label">high</label><item rend="odd_value"/></list></cell></row></table></cell></row></table></cell></row><row><cell rend="wovenodd-col1"><hi rend="label" xml:lang="en">Contained by</hi></cell><cell rend="wovenodd-col2"><ab rend="parent"><ab rend="specChildren"><ab rend="specChild"><seg rend="specChildModule">namesdates: </seg><seg rend="specChildElements"><ref target="#TEI.langKnowledge" rend="link_odd_elementSpec">langKnowledge</ref></seg></ab></ab></ab></cell></row><row><cell rend="wovenodd-col1"><hi rend="label" xml:lang="en">May contain</hi></cell><cell rend="wovenodd-col2"><ab rend="specChildren"><ab rend="specChild"><seg rend="specChildModule">core: </seg><seg rend="specChildElements"><ref target="#TEI.choice" rend="link_odd_elementSpec">choice</ref><c xml:space="preserve"> </c><ref target="#TEI.date" rend="link_odd_elementSpec">date</ref><c xml:space="preserve"> </c><ref target="#TEI.name" rend="link_odd_elementSpec">name</ref><c xml:space="preserve"> </c><ref target="#TEI.note" rend="link_odd_elementSpec">note</ref><c xml:space="preserve"> </c><ref target="#TEI.num" rend="link_odd_elementSpec">num</ref><c xml:space="preserve"> </c><ref target="#TEI.ptr" rend="link_odd_elementSpec">ptr</ref><c xml:space="preserve"> </c><ref target="#TEI.ref" rend="link_odd_elementSpec">ref</ref><c xml:space="preserve"> </c><ref target="#TEI.rs" rend="link_odd_elementSpec">rs</ref><c xml:space="preserve"> </c><ref target="#TEI.title" rend="link_odd_elementSpec">title</ref></seg></ab><ab rend="specChild"><seg rend="specChildModule">namesdates: </seg><seg rend="specChildElements"><ref target="#TEI.orgName" rend="link_odd_elementSpec">orgName</ref><c xml:space="preserve"> </c><ref target="#TEI.persName" rend="link_odd_elementSpec">persName</ref><c xml:space="preserve"> </c><ref target="#TEI.placeName" rend="link_odd_elementSpec">placeName</ref></seg></ab><ab rend="specChild">character data</ab></ab></cell></row><row><cell rend="wovenodd-col1"><hi rend="label" xml:lang="en">Example</hi></cell><cell rend="wovenodd-col2"><egXML xmlns="http://www.tei-c.org/ns/Examples" xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:teix="http://www.tei-c.org/ns/Examples" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en"><langKnown tag="en-GB" level="H">British English</langKnown><langKnown tag="fr" level="M">French</langKnown></egXML></cell></row><row><cell rend="wovenodd-col1"><hi rend="label" xml:lang="en">Schematron</hi></cell><cell rend="wovenodd-col2"><ab xml:space="preserve" rend="pre">
&lt;sch:rule context="//tei:langKnown/@ana"&gt;
&lt;sch:let name="anaValues"
 value="tokenize(., ' ')"/&gt;
&lt;sch:let name="ti"
 value="doc('https://raw.githubusercontent.com/srophe/syriaca/master/documentation/indexes/taxonomyIndex-old.xml')"/&gt;
&lt;sch:let name="languages"
 value="$ti//listURI[@type = 'languages']/uri"/&gt;
&lt;sch:let name="error" value="."/&gt;
&lt;sch:assert test=" every $i in (tokenize(., ' ')) satisfies $i = $languages"
 role="warning"&gt;
 &lt;sch:value-of select="$error"/&gt; is not currently in use. SPEAR has a strong preference for using established
 URIs when possible but appropriate URIs are not always available in the Syriaca.org taxonomy. Please
 consult the taxonomy. If no appropriate keyword exists in the following list, please use your own
 preferred descriptive term: &lt;sch:value-of select="string-join($languages, '; ')"/&gt;.
&lt;/sch:assert&gt;
&lt;/sch:rule&gt;</ab></cell></row><row><cell rend="wovenodd-col1"><hi rend="label" xml:lang="en">Content model</hi></cell><cell rend="wovenodd-col2"><eg xml:space="preserve">
&lt;content&gt;
